{
  "name": "misqabbi-backend",
  "version": "1.0.0",
  "description": "This is the backend for the Misqabbi Store, an e-commerce platform that showcases **Misqabbi**, a women-owned fashion brand specializing in made-to-measure pieces designed exclusively for women.",
  "keywords": [],
  "homepage": "https://github.com/Anorme/misqabbi-backend#readme",
  "bugs": {
    "url": "https://github.com/Anorme/misqabbi-backend/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Anorme/misqabbi-backend.git"
  },
  "license": "ISC",
  "author": "",
  "type": "module",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "dev": "npm run start:dev",
    "docs": "jsdoc -c jsdoc.json",
    "start": "npm run start:prod",
    "start:dev": "cross-env NODE_ENV=development nodemon server.js",
    "start:preview": "cross-env NODE_ENV=preview node server.js",
    "start:staging": "cross-env NODE_ENV=staging node server.js",
    "start:prod": "cross-env NODE_ENV=production node server.js",
    "format:check": "npx prettier --check .",
    "format:fix": "npx prettier --write .",
    "lint": "eslint .",
    "prepare": "husky",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:public": "node --experimental-vm-modules node_modules/jest/bin/jest.js  --testPathIgnorePatterns=tests/integration",
    "test:integration": "node --experimental-vm-modules node_modules/jest/bin/jest.js tests/integration",
    "docker:dev": "docker-compose up --build",
    "docker:dev:d": "docker-compose up -d --build",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f"
  },
  "dependencies": {
    "@logtail/node": "^0.5.5",
    "@logtail/winston": "^0.5.5",
    "@mailchimp/mailchimp_marketing": "^3.0.80",
    "axios": "^1.12.2",
    "bcrypt": "^6.0.0",
    "cloudinary": "^1.41.3",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "cross-env": "^10.0.0",
    "dotenv-flow": "^4.1.0",
    "envalid": "^8.1.0",
    "express": "^5.1.0",
    "express-rate-limit": "^8.2.1",
    "express-slow-down": "^3.0.1",
    "helmet": "^8.1.0",
    "ioredis": "^5.8.2",
    "joi": "^18.0.0",
    "jsonwebtoken": "^9.0.2",
    "mongodb-memory-server": "^10.4.1",
    "mongoose": "^8.16.4",
    "morgan": "^1.10.0",
    "multer": "^2.0.2",
    "multer-storage-cloudinary": "^4.0.0",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-local": "^1.0.0",
    "resend": "^3.5.0",
    "slugify": "^1.6.6",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@eslint/json": "^0.12.0",
    "eslint": "^9.30.1",
    "eslint-config-prettier": "^10.1.5",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "jest": "^30.0.4",
    "jsdoc": "^4.0.4",
    "lint-staged": "^16.1.2",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "supertest": "^7.1.3"
  },
  "jest": {
    "testEnvironment": "node",
    "transform": {}
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.md": [
      "prettier --write"
    ],
    "*.yml": [
      "prettier --write"
    ],
    "*.yaml": [
      "prettier --write"
    ]
  }
}
